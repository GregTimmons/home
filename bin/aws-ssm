#!/usr/bin/env bash
. ~/bin/select_options.sh
. ~/bin/aws-ec2-ls.sh
VALUE=$1
if [ -z "$VALUE" ]; then 
   OPTIONS_STRING=$(aws-ec2-ls)
   readarray -t OPTIONS <<<"${OPTIONS_STRING}"
   select_option "${OPTIONS[@]}"
   CHOICE=$?
   VALUE=${OPTIONS[$CHOICE]}
fi
aws ssm start-session --target $(echo $VALUE | sed 's/|/ /g' | awk '{ print $2 }') 
